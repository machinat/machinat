{
  "compilerOptions": {
    "lib": ["es2020"],
    "module": "Node16",
    "target": "es2020",
    "moduleResolution": "Node16",
    "strictNullChecks": true,
    "esModuleInterop": true,
    "jsx": "react",
    "jsxFactory": "Sociably.createElement",
    "jsxFragmentFactory": "Sociably.Fragment",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "types": ["node"],
    "baseUrl": "."
  }
}
